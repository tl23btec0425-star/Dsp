#include<stdio.h>
#include "input"
#include "impulse"
int y[9];
main()
{
    int n,k;
    FILE *fptr=fopen("output.txt", "w");
    for(n=0; n<9; n++)
    {
        y[n]=0;
        for(k=0; k<=n; k++)
        {
            y[n]=y[n]+x[k]*h[n-k];
        }
        printf("%d\t",y[n]);
        fprintf(fptr, "%d\t",y[n]);
    }
}
